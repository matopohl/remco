{
    "env": {
        "browser": true,
        "node": true,
        "amd": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended"
    ],
    "globals": {
        "$": true,
        "require": true,
        "process": true,
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "requireConfigFile": false,
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "ignorePatterns": ["node_modules/", "public/"],
    "rules": {
    }
}
